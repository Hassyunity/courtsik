= form_with(model: post) do |form|
  .field
    = form.label :name
    = form.text_field :name
  .field
    = form.label :content
    = form.text_area :content
  .field
    = form.label :user_id
    = form.text_field :user_id
  .field
    = form.label :image
    = form.file_field :image
  .actions
    = form.submit
